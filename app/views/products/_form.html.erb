  <%= simple_form_for @product do |f| %>
    <%= f.input :title, placeholder: "Name" %>
    <%= f.input :address, placeholder: "Address" %>
    <%= f.input :city, placeholder: "City" %>
    <%= f.input :price, placeholder: "Price" %>
    <%= f.input :description, placeholder: "Tell us a bit about your product" %>
    <%= f.input :capacity, placeholder: "How people can you " %>
    <%= f.input :category, placeholder: "How would you define your place ?" %>
    <%= f.input :photos, as: :attachinary %>
    <%= f.submit "Add your product", class: "btn btn-primary" %>
  <% end %>



<%= content_for(:after_js) do %>
  <script src="https://maps.googleapis.com/maps/api/js?libraries=places&amp;key=AIzaSyAe4PVk38pNIO3t77HZnIT6f_8b_k30BCs"></script>
<% end %>



